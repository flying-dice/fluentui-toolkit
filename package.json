{
  "name": "@flying-dice/fluentui-toolkit",
  "version": "0.1.8",
  "description": "Fluent UI Extensions Toolkit",
  "license": "MIT",
  "author": "Jonathan Turnock <Jonathan.Turnock@outlook.com>",
  "homepage": "https://flying-dice.github.io/fluentui-toolkit/",
  "repository": "https://github.com/flying-dice/fluentui-toolkit",
  "main": "dist/index.js",
  "scripts": {
    "commit": "cz",
    "release": "standard-version",
    "prettier": "prettier src -c",
    "prettier:fix": "prettier src -w",
    "clean": "rimraf dist storybook-static",
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "node scripts/generateDocs",
    "ci": "yarn install && yarn prettier && cross-env CI=true yarn build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "dependencies": {
    "@fluentui/react": "^7.158.1",
    "@fluentui/react-hooks": "^8.0.0-beta.0",
    "@fluentui/react-theme-provider": "^0.18.1",
    "@flying-dice/commons": "^0.1.1",
    "@prezly/react-promise-modal": "^0.2.0",
    "decimal.js": "^10.2.1",
    "hex-to-rgba": "^2.0.1",
    "lodash": "^4.17.20",
    "matcher": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-responsive": "^8.2.0",
    "styled-components": "^5.2.1",
    "ts-key-enum": "^2.0.7",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-responsive": "^8.0.2",
    "@types/styled-components": "^5.1.7",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "gh-pages": "^3.1.0",
    "handlebars": "^4.7.6",
    "husky": "^4.3.8",
    "i18n-iso-countries": "^6.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn audit && yarn prettier"
    }
  }
}
